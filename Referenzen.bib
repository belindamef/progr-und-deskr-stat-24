@article{ostwald_information_2010,
  title = {An Information Theoretic Approach to {{EEG}}\textendash{{fMRI}} Integration of Visually Evoked Responses},
  author = {Ostwald, Dirk and Porcaro, Camillo and Bagshaw, Andrew P.},
  year = {2010},
  month = jan,
