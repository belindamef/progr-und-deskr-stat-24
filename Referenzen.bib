@article{worsley_estimating_1995,
  title = {Estimating the {{Number}} of {{Peaks}} in a {{Random Field Using}} the {{Hadwiger Characteristic}} of {{Excursion Sets}}, with {{Applications}} to {{Medical Images}}},
  author = {Worsley, K. J.},
