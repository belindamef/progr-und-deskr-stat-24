@article{acunzo_systematic_2012,
  title = {Systematic Biases in Early {{ERP}} and {{ERF}} Components as a Result of High-Pass Filtering},
  author = {Acunzo, David J. and MacKenzie, Graham and {van Rossum}, Mark C.W.},
