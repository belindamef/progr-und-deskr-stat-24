@article{kennedy-shaffer_05_2019,
  title = {Before {$<$} 0.05 to {{Beyond}}{$<$} 0.05: {{Using History}} to {{Contextualize}} p -{{Values}} and {{Significance Testing}}},
  shorttitle = {Before {\emph{p}} {$<$} 0.05 to {{Beyond}} {\emph{p}} {$<$} 0.05},
