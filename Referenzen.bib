@article{frei_localization_2001,
  title = {Localization of {{MDMA}}-Induced Brain Activity in Healthy Volunteers Using Low Resolution Brain Electromagnetic Tomography ({{LORETA}})},
  author = {Frei, Edi and Gamma, Alex and {Pascual-Marqui}, Roberto and Lehmann, Dietrich and Hell, Daniel and Vollenweider, Franz X.},
  year = {2001},
